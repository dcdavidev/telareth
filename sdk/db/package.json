{
  "name": "@telareth/db",
  "version": "0.1.0",
  "description": "Telareth Database sdk",
  "keywords": [
    "telareth",
    "telareth-db",
    "db",
    "database",
    "api",
    "rest",
    "rest-api",
    "api-gateway",
    "gateway"
  ],
  "license": "MIT",
  "homepage": "https://github.com/telareth/telareth/blob/main/sdk/db",
  "repository": {
    "type": "git",
    "url": "https://github.com/telareth/telareth.git",
    "directory": "sdk/db"
  },
  "bugs": {
    "url": "https://github.com/telareth/telareth/issues"
  },
  "scripts": {},
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@telareth/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./helpers": {
      "types": "./dist/helpers/index.d.ts",
      "import": "./dist/helpers/index.js",
      "default": "./dist/helpers/index.js"
    },
    "./middlewares": {
      "types": "./dist/middlewares/index.d.ts",
      "import": "./dist/middlewares/index.js",
      "default": "./dist/middlewares/index.js"
    },
    "./queries": {
      "types": "./dist/queries/index.d.ts",
      "import": "./dist/queries/index.js",
      "default": "./dist/queries/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js",
      "default": "./dist/schemas/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "default": "./dist/types/index.js"
    },
    "./consts": {
      "types": "./dist/consts.d.ts",
      "import": "./dist/consts.js",
      "default": "./dist/consts.js"
    },
    "./init": {
      "types": "./dist/init.d.ts",
      "import": "./dist/init.js",
      "default": "./dist/init.js"
    }
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "redis": "^5.8.2",
    "tslib": "^2.3.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.9"
  },
  "nx": {
    "tags": [
      "scope:db",
      "runtime:sdk"
    ],
    "targets": {
      "clean": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "sdk/db",
          "commands": [
            "rimraf dist"
          ]
        }
      },
      "install": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npm install"
          ]
        },
        "dependsOn": [
          "^clean"
        ]
      },
      "lint:fmt": {
        "executor": "nx:run-commands",
        "options": {
          "command": [
            "eslint --fix ."
          ]
        },
        "dependsOn": [
          "^install"
        ]
      }
    }
  }
}
