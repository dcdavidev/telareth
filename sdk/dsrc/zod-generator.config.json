{
  "moduleFormat": "esm",
  "importFileExtension": "js",
  "useMultipleFiles": true,
  "mode": "full",
  "pureModels": true,
  "variants": {
    "pure": { "enabled": true, "suffix": ".model" },
    "input": {
      "enabled": true,
      "suffix": ".input",
      "excludeFields": ["password"]
    },
    "result": { "enabled": true, "suffix": ".result" }
  },
  "emit": {
    "crud": false
  }
}
