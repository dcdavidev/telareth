{
  "name": "@telareth/sdk",
  "version": "0.0.1",
  "description": "Telareth sdk",
  "keywords": [
    "telareth",
    "api",
    "rest",
    "rest-api",
    "api-gateway",
    "gateway"
  ],
  "license": "MIT",
  "homepage": "https://github.com/telareth/telareth/blob/main/sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/telareth/telareth.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/telareth/telareth/issues"
  },
  "scripts": {},
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "dist",
    "docker",
    ".env",
    "ecosystem.config.cjs",
    "prisma.config.mjs"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@telareth/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "@prisma/client": "^6.16.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "morgan": "^1.10.1",
    "tslib": "^2.3.0",
    "zod": "^4.1.8"
  },
  "nx": {
    "tags": [
      "scope:sdk",
      "runtime:core"
    ],
    "targets": {
      "db:docker:up": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "sdk",
          "commands": [
            "docker compose -f docker/docker-compose.yml up -d",
            "sleep 5"
          ],
          "parallel": false
        }
      },
      "db:docker:down": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "sdk",
          "commands": [
            "docker compose -f docker/docker-compose.yml down"
          ]
        }
      },
      "generate:db:credentials": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "sdk",
          "commands": [
            "telareth g db credentials"
          ]
        }
      },
      "migrate:dev": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "sdk",
          "commands": [
            "prisma migrate dev --name \"$(npm telareth g name -t 2 -s '')\""
          ]
        }
      },
      "migrate:dev:docker": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "sdk",
          "commands": [
            "nx run @telareth/sdk:db:docker:up",
            "prisma migrate dev --name \"$(npm telareth g name -t 2 -s '')\""
          ],
          "parallel": false
        }
      },
      "generate:db:types": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "sdk",
          "commands": [
            "prima generate"
          ]
        }
      },
      "dev:pm2": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "sdk",
          "commands": [
            "pm2 start ecosystem.config.cjs"
          ]
        }
      },
      "reload:pm2": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "sdk",
          "commands": [
            "pm2 reload ecosystem.config.cjs"
          ]
        }
      }
    }
  },
  "devDependencies": {
    "@telareth/cli": "^0.7.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "prisma": "^6.16.1",
    "typescript": "^5.9.2"
  }
}
