{
  "name": "@telareth/eslint-plugin",
  "version": "0.11.2",
  "description": "Telareth ESLint plugin and configs",
  "type": "module",
  "keywords": [
    "telareth",
    "eslint",
    "eslint-plugin",
    "eslint-config",
    "eslint-configs"
  ],
  "license": "MIT",
  "homepage": "https://github.com/telareth/telareth/tree/main/tools/eslint-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/telareth/telareth.git",
    "directory": "tools/eslint-plugin"
  },
  "bugs": {
    "url": "https://github.com/telareth/telareth/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@telareth/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "packageManager": "pnpm@10.15.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@eslint/js": "^9.35.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^57.0.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.4.0",
    "jsonc-eslint-parser": "^2.4.0",
    "tslib": "^2.3.0",
    "typescript-eslint": "^8.43.0"
  },
  "peerDependencies": {
    "eslint": "^9.35.0"
  },
  "devDependencies": {
    "eslint": "^9.35.0",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "eslint-plugin-n": "^17.21.3"
  },
  "nx": {
    "tags": [
      "scope:tools",
      "type:devtool",
      "tool:eslint",
      "runtime:devtool"
    ],
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm run eslint --fix .",
          "pnpm dlx rimraf ./dist"
        ],
        "parallel": false
      }
    },
    "targets": {
      "build": {
        "dependesOn": [
          "clean"
        ]
      }
    }
  }
}
